<%- include("partials/header"); -%>
    <h1>Home</h1>
    <p>
        <%= homeStartingContent %>
    </p>
    <% posts.forEach(function(post) { %>
        <h2>
            <%= post.postTitle %>
        </h2>
        <p>
            <%= post.postBody.substring(0, 50) + "..." %>
            <a href="/posts/<%= post.postTitle %>">Read More</a>
        </p>
        <% }) %>


            <%- include("partials/footer"); -%>